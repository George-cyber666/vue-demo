<!--pages/activity1/my/myintegral.wxml-->
<navigation-bar default-data="{{defaultData}}"></navigation-bar>
<view class="content">
  <view class="home">
    <view class="music {{isMusic ? 'musicon':''}}" catchtap="music">
      <view class="music-li"></view>
      <view class="music-li"></view>
      <view class="music-li"></view>
    </view>
    <view class="myinbox">
      <image src="{{imgsrc}}activity/myinicon1.png"></image>
      <view>步数积分</view>
      <image src="{{imgsrc}}activity/myinxing.png"></image>
      <view>{{dota.stepsScore}}</view>
    </view>
    <view class="myinbox">
      <image src="{{imgsrc}}activity/myinicon2.png"></image>
      <view>签到积分</view>
      <image src="{{imgsrc}}activity/myinxing.png"></image>
      <view>{{dota.qdScore}}</view>
    </view>
    <view class="myinbox">
      <image src="{{imgsrc}}activity/myinicon3.png"></image>
      <view>问卷积分</view>
      <image src="{{imgsrc}}activity/myinxing.png"></image>
      <view>{{dota.wjScore}}</view>
    </view>
    <view class="myinbox">
      <image src="{{imgsrc}}activity/myinicon4.png"></image>
      <view>视频积分</view>
      <image src="{{imgsrc}}activity/myinxing.png"></image>
      <view>{{dota.videoScore}}</view>
    </view>
    <view class="myinbox">
      <image src="{{imgsrc}}activity/myinicon5.png"></image>
      <view>游戏积分</view>
      <image src="{{imgsrc}}activity/myinxing.png"></image>
      <view>{{dota.gameScore}}</view>
    </view>
  </view>
</view>

<view class="bar">
  <view bindtap="clicktoindex" class="baricon1">
    <image src="{{imgsrc}}baricon4.png"></image>
    <view>首页</view>
  </view>
  <view bindtap="clicktoac" class="baricon2">
    <image src="{{imgsrc}}baricon5.png"></image> 
    <view>活动</view>
  </view>
  <view bindtap="clicktomy" class="baricon3">
    <image src="{{imgsrc}}baricon6.png"></image>
    <view>我的</view>
  </view>
</view>